{
  "extends": ["esnext", "plugin:react/recommended"],
  "rules": {
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "import/no-unresolved": "off"
  },
  "globals": {
    "_": true,
    "expect": true,
    "describe": true,
    "it": true,
    "document": true,
    "beforeEach": true
  }
}
